{
  "manifest_version": 2,
  "name": "NewTab Color Void",
  "description": "This extension replaces the default new tab window with a single-color (customizable in options) screen.",
  "version": "1.0",
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true },
  "chrome_url_overrides" : { "newtab": "tab.html" },
  "permissions": [ "storage" ],
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "96": "icon96.png",
             "128": "icon128.png" }
}